package com.xoriant.beans;

import java.util.List;
import java.util.Set;

public class FlowerBouquet {
	private String name;
	private int id;
	private double cost;
	private List<String> flowers;
	private Set<String> flowersSet;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public double getCost() {
		return cost;
	}
	public void setCost(double cost) {
		this.cost = cost;
	}
	public List<String> getFlowers() {
		return flowers;
	}
	public void setFlowers(List<String> flowers) {
		this.flowers = flowers;
	}
	public Set<String> getFlowersSet() {
		return flowersSet;
	}
	public void setFlowersSet(Set<String> flowersSet) {
		this.flowersSet = flowersSet;
	}
	public FlowerBouquet() {
		super();
	}
	
	
}
