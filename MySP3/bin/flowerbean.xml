<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.1.xsd">

	<bean class="com.xoriant.beans.FlowerBouquet" id="flowerBouquet"> 
		<property name="name" value="bouquet1"></property>
		<property name="cost" value="1500"></property>
		<property name="id" value="1"></property>
		<property name="flowers">
			<list>
				<value>Roses</value>
				<value>Tulips</value>
				<value>Dandelions</value>
				<value>Roses</value>
			</list>
		</property>
		<property name="flowersSet">
			<set>
				<value>Roses</value>
				<value>Roses</value> <!-- Duplicates not allowed in set-check output -->
				<value>Tulips</value>
			</set>
		</property>
	</bean>
</beans>
